{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "installCommand": "npm install",
    "framework": "nextjs",
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "NODE_ENV": "production"
    },
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/admin/auth/login",
        "dest": "/api/admin/auth/login"
      },
      {
        "src": "/api/test",
        "dest": "/api/test"
      },
      {
        "src": "/api/status",
        "dest": "/api/status"
      }
    ],
    "functions": {
      "api/**/*": {
        "memory": 1024
      }
    }
  }